<Window x:Class="TrainR_Admin.RemoveDeparture"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TrainR_Admin"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="RemoveDeparture" Height="132" Width="275">
    <Grid>
        <Label Content="Name" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="27"/>
        <ComboBox Name="DepartureName" Height="20" Margin="10,37,10,0" VerticalAlignment="Top"/>
        <Button Content="Cancel" IsCancel="True" HorizontalAlignment="Left" Margin="10,0,0,10" Width="50" Height="20" VerticalAlignment="Bottom"/>
        <Button Content="Remove" IsDefault="True" Margin="0,0,10,10" Click="Button_Click" RenderTransformOrigin="0.491,0.49" Height="20" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="50"/>
    </Grid>
</Window>
