<Window x:Class="TrainR_Admin.RemoveUser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TrainR_Admin"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="RemoveUser" Height="130" Width="316">
    <Grid>
        <Label Content="User:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <ComboBox Name="Users" HorizontalAlignment="Left" Margin="10,36,0,0" VerticalAlignment="Top" Width="212"/>
        <Button IsCancel="True" Content="Cancel" Margin="10,0,0,10" HorizontalAlignment="Left" Width="50" Height="20" VerticalAlignment="Bottom"/>
        <Button IsDefault="True" Content="Remove" Margin="0,0,10,10" HorizontalAlignment="Right" Width="50" Click="Button_Click" Height="20" VerticalAlignment="Bottom"/>
    </Grid>
</Window>
